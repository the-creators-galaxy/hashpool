{
  // Sample & Default Configuration
  // for Integration Tests

  // The Mirror Node URL to connect to, this will
  // establish which network the unit tests will be
  // executed against.  The test framework will select
  // a gossip node randomly from the mirror nodes'
  // address book.
  "mirror": "",

  // The test account that having
  // hBar sufficient to pay for the
  // test messages sent to the test network.
  "account": {
    // Realm of the test account
    "realm": 0,
    // Shard of the test account
    "shard": 0,
    // Account number of the test account
    "number": 0,
    // Public key of the test account 
    "publicKey": "",
    // Private key of the test account used
    // to sign requests sent to the server.
    // DO NOT STORE HERE, use Developer
    // Secrets or Environment variables.
    "privateKey": ""
  }
}